<odoo>
    <record id="view_employee_employee_graph" model="ir.ui.view">
        <field name="name">employee.employee.graph</field>
        <field name="model">employee.employee</field>
        <field name="arch" type="xml">
            <graph string="Employees Analysis">
                <field name="name" type="row"/>
                <field name="age" type="measure"/>
            </graph>
        </field>
    </record>

    <record id="view_employee_employee_pivot" model="ir.ui.view">
        <field name="name">employee.employee.pivot</field>
        <field name="model">employee.employee</field>
        <field name="arch" type="xml">
            <pivot string="Employees Analysis">
                <field name="name" type="row"/>
                <field name="age" type="measure"/>
            </pivot>
        </field>
    </record>
</odoo>