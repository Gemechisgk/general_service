<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Property Management -->
        <record id="seq_property_management" model="ir.sequence">
            <field name="name">Property Management</field>
            <field name="code">property.management</field>
            <field name="prefix">PROP/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Rental Agreement -->
        <record id="seq_rental_agreement" model="ir.sequence">
            <field name="name">Rental Agreement</field>
            <field name="code">rental.agreement</field>
            <field name="prefix">RA/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_employee_registration" model="ir.sequence">
            <field name="name">Employee Registration Sequence</field>
            <field name="code">employee.registration.sequence</field>
            <field name="prefix">EMP/</field>
            <field name="padding">4</field>
        </record>

        <!-- Sequence for fuel request -->
        <record id="seq_fuel_request" model="ir.sequence">
            <field name="name">Fuel Request Sequence</field>
            <field name="code">fuel.request.sequence</field>
            <field name="prefix">FUEL/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo> 